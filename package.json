{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "prod": "run-s prod:build prod:server",
    "prod:server": "cross-env NODE_ENV=production node bin/run",
    "prod:build": "webpack --config webpack.config.prod",
    "dev": "npm-run-all dev:clean -p dev:server dev:open",
    "dev:server": "cross-env NODE_ENV=development nodemon bin/run",
    "dev:open": "wait-on http://localhost:3000 && opener http://localhost:3000",
    "dev:clean": "rimraf public"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "co": "^4.6.0",
    "css-loader": "^0.26.2",
    "debug": "^2.2.0",
    "ect": "^0.5.9",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^2.1.0",
    "jquery": "^3.1.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.0.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^1.1.1",
    "koa-logger": "^1.3.0",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.0.0",
    "koa-static": "^2.0.0",
    "koa-views": "^5.0.1",
    "koa-webpack-middleware": "^1.0.3",
    "nodemon": "^1.8.1",
    "postcss-loader": "^1.3.3",
    "runkoa": "^1.3.1",
    "style-loader": "^0.13.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "cross-env": "^5.0.5",
    "npm-run-all": "^4.0.2",
    "opener": "^1.4.3",
    "rimraf": "^2.6.1",
    "wait-on": "^2.0.2"
  }
}
