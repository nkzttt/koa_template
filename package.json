{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "prod": "run-s prod:build prod:server",
    "prod:server": "cross-env NODE_ENV=production node bin/run",
    "prod:build": "npm run build",
    "dev": "npm-run-all build -p dev:*",
    "dev:server": "cross-env NODE_ENV=development nodemon bin/run",
    "dev:watch": "wait-on http://localhost:3000 && node scripts/watch",
    "build": "run-p build:*",
    "build:js": "node scripts/js",
    "build:css": "node scripts/css",
    "build:asset": "node scripts/asset",
    "prebuild": "rimraf public"
  },
  "dependencies": {
    "co": "^4.6.0",
    "debug": "^2.2.0",
    "ect": "^0.5.9",
    "jquery": "^3.1.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.0.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^1.1.1",
    "koa-logger": "^1.3.0",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.0.0",
    "koa-static": "^2.0.0",
    "koa-views": "^5.0.1",
    "nodemon": "^1.8.1",
    "runkoa": "^1.3.1"
  },
  "devDependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.13",
    "browserify": "^14.4.0",
    "cross-env": "^5.0.5",
    "fs-extra": "^4.0.1",
    "globby": "^6.1.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "stylus": "^0.54.5",
    "uglifyify": "^4.0.3",
    "wait-on": "^2.0.2"
  }
}
